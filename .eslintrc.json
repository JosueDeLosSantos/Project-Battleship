{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base",
    "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "import/prefer-default-export": "off",
        "import/no-cycle": [2, { "maxDepth": 1 }],
        "no-plusplus": 0,
        "no-use-before-define": 0,
        "eslint no-param-reassign": 0
    }
}
